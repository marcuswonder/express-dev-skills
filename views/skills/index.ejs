<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>

<body style="font-family: Arial">
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
            <a href="/skills/<%= s.skill %>">
            <%= s.skill %>
            </a>
            - 
            <%= s.skill > 5 ? 'proficient' : 'not yet proficient' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>